  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.1",
    "if-env": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-terser-js": "^1.0.2",
    "webpack": "^5.74.0",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.4",
    "webpack-cli": "^5.0.0",
    "webpack-pwa-manifest": "^4.3.0"
  }
}
